{
    "name":"Stickyeyes Intranet Notifications",
    "update_url": "https://raw.githubusercontent.com/MatthewShields/intranet-extension/master/intranet-chrome/updates.xml",
    "manifest_version":2,
    "version":"1.1",
    "background":{
        "scripts":["jquery.js", "notification.js"]
    },
    "browser_action":{
        "default_icon":"logo.png"
    },
    "permissions":[
        "notifications",
        "background",
        "tabs",
        "http://intranet.stickyeyes.com/",
        "webRequest", "webRequestBlocking"
    ],
    "web_accessible_resources":[
        "logo.png"
    ],
    "content_scripts": [ {
        "js": [ "jquery.js" ],
        "matches": [ "http://*/*", "https://*/*"]
    }]
}
